{% extends 'base.html.twig' %}

{% block title %}Celconsulting{% endblock %}


{% block body %}
    <div class="contact__body">
        <div class="header__top__own">
            <div class="linter__top">
                <a href="">Celconsulting</a>
            </div>

        </div>
        <div class="map">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3196.1347979447337!2d10.24235671523431!3d36.767333579955356!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x12fd360996ad6c31%3A0xc7f2d37230d0dcd2!2sSidi%20Rezig!5e0!3m2!1sfr!2stn!4v1661409774146!5m2!1sfr!2stn" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

        </div>
        <div class="form__contact">
            <div class="content__liner">
                <h1 class="form__text">Contactez-nous</h1>
                {{ form_start(Contact, { 'attr': {'autocomplete': 'off'} })  }}

                {{ form_widget(Contact.Nom, { 'attr': {'class': "span12",
                    'placeholder': "Nom Complet",'autofocus':"on"} }) }}

                {{ form_widget(Contact.Email, { 'attr': {'class': "span12",
                    'placeholder': "Email",'autofocus':"on"} }) }}
                {{ form_widget(Contact.numero, { 'attr': {'class': "span12",
                    'placeholder': "Numero Telephone",'autofocus':"on"} }) }}
                {{ form_widget(Contact.message, { 'attr': {'class': "span12",
                    'placeholder': "Message",'autofocus':"on"} }) }}

                {{ form_end(Contact) }}
            </div>
        </div>

        <div class="footer__contact">
            <div class="content__footer">
                <div class="element__foot">
                    <h2>Email</h2>
                    <p>contactcelconsulting@.com</p>
                    <p>Celconsulting@gmail.com</p>
                </div>
                <div class="element__foot">
                        <h2>Telephone</h2>
                    <p>+216 52811997</p>
                    <p>+243 52811997</p>
                </div>
                <div class="element__foot">
                    <h2>Adresse</h2>
                    <p>Megrine</p>
                    <p>sidi rezig</p>
                </div>
            </div>
        </div>

    </div>

{% endblock %}